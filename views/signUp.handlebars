<!doctype html>

<html>

<head>
    <title>Sign Up Page</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/signUp.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="https://kit.fontawesome.com/610d507d86.js" crossorigin="anonymous"></script>
</head>

<body>
    <div class="container">
        <h1>Baby Step</h1>
        <div>
            <h2>EMAIL:</h2>
            <input type='text' class="form-control" id="email" placeholder="example@gmai.com" name='email'>
        </div>
        <div>
            <h2>PASSWORD:</h2>
            <input type="text" class="form-control" id="password" placeholder="abc123!@#" name="password">
        </div>
        <button type="button" onclick="location.href='http://localhost:3000/signIn'">
            SIGN UP
        </button>
    </div>
</body>

</html>